" Vim Tmux Runner - Connect Vim and Tmux to allow running lines and commands

Plug 'christoomey/vim-tmux-runner'

let g:vtr_filetype_runner_overrides = {
            \ 'php': 'php -f {file}'
            \}

nnoremap <leader>ror :VtrOpenRunner {'orientation': 'h', 'percentage': 25}<CR>
nnoremap <leader>rkr :VtrKillRunner<CR>
" vim:ft=vim
